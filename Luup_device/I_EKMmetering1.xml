<?xml version="1.0" encoding="UTF-8"?>
<implementation>
   <settings>
      <protocol>raw</protocol>
      <handleChildren>1</handleChildren>
   </settings>
   <files>L_EKMmetering1.lua</files>
   <incoming>
      <lua>processIncoming(lul_data)</lua>
   </incoming>
   <startup>luaStartUp</startup>
   <actionList>
     <action>
      <serviceId>urn:a-lurker-com:serviceId:EKMmetering1</serviceId>
      <name>Polling</name>
      <run>
        polling(lul_settings.PollEnable)
      </run>
    </action>
     <action>
      <serviceId>urn:a-lurker-com:serviceId:EKMmetering1</serviceId>
      <name>SetMeteringFunction</name>
      <run>
        setMeteringFunction(lul_settings.MeteringFunction)
      </run>
    </action>
     <action>
      <serviceId>urn:a-lurker-com:serviceId:EKMmetering1</serviceId>
      <name>SetMeterSerialNumber</name>
      <run>
        setMeterSerialNumber(lul_settings.MeterSerialNumber)
      </run>
    </action>
     <action>
      <serviceId>urn:a-lurker-com:serviceId:EKMmetering1</serviceId>
      <name>SetUseSecondMeter</name>
      <run>
        setUseSecondMeter(lul_settings.UseSecondMeter)
      </run>
    </action>
     <action>
      <serviceId>urn:a-lurker-com:serviceId:EKMmetering1</serviceId>
      <name>SetMeterModel</name>
      <run>
        setMeterModel(lul_settings.MeterModel)
      </run>
    </action>
  </actionList>
</implementation>
